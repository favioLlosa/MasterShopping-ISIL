<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="buttom_primary">
        <item name="backgroundTint">@null</item>
        <item name="android:background">@drawable/btn_primary</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:paddingVertical">8dp</item>
    </style>

    <style name="buttom_options_profile">
        <item name="android:drawableRight">@drawable/ic_baseline_arrow_forward_ios_24</item>
        <item name="backgroundTint">@color/white</item>
        <item name="cornerRadius">80dp</item>
        <item name="android:textColor">@color/black</item>
        <item name="textAllCaps">false</item>
        <item name="android:textAlignment">textStart</item>
    </style>

    <style name="back_to">
        <item name="android:drawableLeft">@drawable/ic_baseline_arrow_back_ios_24</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">20sp</item>
    </style>

    <style name="navigation_round">
        <item name="android:background">@drawable/nav_round</item>
        <item name="background">@drawable/nav_round</item>
    </style>
</resources>